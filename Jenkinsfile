pipeline {
    agent any

    stages {
       stage('Hello') {
            steps {
                echo 'This process is hellowing the project'
            }
        }
        stage('Build') {
            steps {
                echo 'This process is building the project'
            }
        }
           stage('Testing') {
            steps {
                echo 'This process is Testing the project'
            }
        }
           stage('Release') {
            steps {
                echo 'This process is Releasing the project'
            }
        }
           stage('Deploy') {
            steps {
                echo 'This process is Deploying the project'
            }
        }
    }
}
